{
    "name": "dragon-code/iconify-ide",
    "description": "Easily stylize your IDE interface by adding branded project icons",
    "license": "MIT",
    "type": "project",
    "keywords": [
        "brand",
        "brands",
        "cli",
        "console",
        "icon",
        "iconify",
        "ide",
        "laravel",
        "moonshine"
    ],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@dragon-code.pro",
            "homepage": "https://dragon-code.pro"
        }
    ],
    "support": {
        "issues": "https://github.com/TheDragonCode/iconify-ide/issues",
        "source": "https://github.com/TheDragonCode/iconify-ide"
    },
    "funding": [
        {
            "type": "boosty",
            "url": "https://boosty.to/dragon-code"
        },
        {
            "type": "yoomoney",
            "url": "https://yoomoney.ru/to/410012608840929"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*"
    },
    "require-dev": {
        "dragon-code/support": "^6.15",
        "laravel-zero/framework": "^11.36.1",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "DragonCode\\IconifyIde\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "builds/iconify"
    ],
    "config": {
        "allow-plugins": {
            "dragon-code/codestyler": true,
            "ergebnis/composer-normalize": true,
            "pestphp/pest-plugin": true,
            "symfony/thanks": true
        },
        "optimize-autoloader": true,
        "platform": {
            "php": "8.2.26"
        },
        "preferred-install": "dist",
        "sort-packages": true
    }
}
